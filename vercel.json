{
    "builds": [
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/frontend/index.html"
        }
    ],
    "rewrites": [
        { "source": "/loginform", "destination": "/api/loginform" }
    ]
}  